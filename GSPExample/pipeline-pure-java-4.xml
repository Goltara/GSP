
<pipeline>
    <namespace ex="fr.prima.gspexample"/>

    <m id="tick1" type="ex:Counter" maxCount="10" period="300" warmupDelay="1050"/>
    <m id="tick2" type="ex:Counter" maxCount="17" period="300"/>
    <m id="cons" type="ex:Console"/>

    <c chain="tick1 - cons"/>
    <c chain="tick2 - warning#cons"/>

</pipeline>

